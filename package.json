    "dotenv": "^17.2.0",
    "eslint": "^9.31.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-sonarjs": "^3.0.4",
    "ethers": "^6.15.0",
    "glob": "^11.0.3",
    "jsbi": "^4.3.2",
    "jsdom": "^26.1.0",
    "knip": "^5.61.3",
    "nodemon": "^3.1.10",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.3",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.7",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@4.9.2",
