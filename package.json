{
  "name": "@xyo-network/plugins",
  "version": "5.1.1",
  "description": "Typescript/Javascript Plugins for XYO Platform",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "git+https://github.com/XYOracleNetwork/plugins/issues",
    "email": "support@xyo.network"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XYOracleNetwork/plugins.git"
  },
  "license": "LGPL-3.0-only",
  "author": {
    "name": "XYO Development Team",
    "email": "support@xyo.network",
    "url": "https://xyo.network"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/neutral/index.d.ts",
      "default": "./dist/neutral/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "module": "dist/neutral/index.mjs",
  "types": "dist/neutral/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "workspaces": [
    "packages/**/*"
  ],
  "scripts": {
    "build": "xy build && xy statics",
    "build-tests": "tsc --noEmit --lib dom,esnext",
    "build-typedoc-site": "typedoc",
    "compile": "./scripts/clear-scrollback-buffer.sh && yarn xy compile",
    "coverage": "yarn vitest --coverage --forceExit",
    "deploy": "xy deploy",
    "lint-pkg": "npmPkgJsonLint .",
    "test": "vitest run"
  },
  "dependencies": {
    "@xyo-network/payload-plugins": "workspace:~",
    "@xyo-network/payloadset-plugins": "workspace:~"
  },
  "devDependencies": {
    "@ethersproject/address": "~5.8.0",
    "@ethersproject/bignumber": "~5.8.0",
    "@stylistic/eslint-plugin": "~5.2.3",
    "@types/supertest": "~6.0.3",
    "@typescript-eslint/eslint-plugin": "~8.41.0",
    "@typescript-eslint/parser": "~8.41.0",
    "@vitest/coverage-v8": "~3.2.4",
    "@xylabs/eslint-config-flat": "~7.1.7",
    "@xylabs/express": "~5.0.11",
    "@xylabs/forget": "~5.0.11",
    "@xylabs/platform": "~5.0.11",
    "@xylabs/ts-scripts-yarn3": "~7.1.7",
    "@xylabs/tsconfig": "~7.1.7",
    "@xylabs/tsconfig-dom": "~7.1.7",
    "@xylabs/vitest-extended": "~5.0.11",
    "@xyo-network/account": "~5.1.2",
    "@xyo-network/archivist-memory": "~5.1.2",
    "@xyo-network/boundwitness-builder": "~5.1.2",
    "@xyo-network/hash": "~5.1.2",
    "@xyo-network/manifest": "~5.1.2",
    "@xyo-network/manifest-wrapper": "~5.1.2",
    "@xyo-network/module-factory-locator": "~5.1.2",
    "@xyo-network/node-memory": "~5.1.2",
    "@xyo-network/payload-builder": "~5.1.2",
    "@xyo-network/wallet": "~5.1.2",
    "chalk": "~5.6.0",
    "dotenv": "~17.2.1",
    "eslint": "~9.34.0",
    "eslint-import-resolver-typescript": "~4.4.4",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-sonarjs": "~3.0.5",
    "ethers": "~6.15.0",
    "glob": "~11.0.3",
    "jsbi": "~4.3.2",
    "jsdom": "~26.1.0",
    "knip": "~5.63.1",
    "nodemon": "~3.1.10",
    "reflect-metadata": "~0.2.2",
    "rimraf": "~6.0.1",
    "supertest": "~7.1.4",
    "tslib": "~2.8.1",
    "typedoc": "~0.28.13",
    "typescript": "~5.9.2",
    "vitest": "~3.2.4"
  },
  "packageManager": "yarn@4.9.2",
  "volta": {
    "node": "22.14.0",
    "yarn": "1.22.22"
  },
  "publishConfig": {
    "access": "public"
  }
}
